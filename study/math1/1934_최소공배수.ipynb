    "# input\n",
    "t = int(input())\n",
    "\n",
    "def gcd(a,b):\n",
    "    if a%b==0:\n",
    "        return b\n",
    "    else:\n",
    "        return gcd(b,a%b)\n",
    "\n",
    "def lcm(a,b):\n",
    "    return a*b//gcd(a,b)\n",
    "\n",
    "for i in range(t):\n",
    "    a,b=map(int,input().split())\n",
    "    print(lcm(a,b))"
   ]
  }
